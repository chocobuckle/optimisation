{
  "name": "udacity-p4",
  "version": "1.0.0",
  "description": "A website performance/rendering optimisation, submitted as a project for the Udacity Front-End Developer Nanodegree program.",
  "scripts": {
    "resizePizzaImage": "node ./src/js/resizeImages.js",
    "compressImages": "node ./src/js/compressImages.js",
    "buildImages": "npm run resizePizzaImage && npm run compressImages",
    "minifyCSS": "cleancss -o ./dist/css/style.min.css ./src/css/style.css",
    "inlineCSS": "inline-css -i ./index-pre-inlined-CSS.html -o ./index.html",
    "buildCSS": "npm run minifyCSS && npm run inlineCSS",
    "start": "npm run buildImages && npm run buildCSS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chocobuckle/performance-optimisation.git"
  },
  "main": "./src/js/index.js",
  "author": "Conor Doyle",
  "license": "ISC",
  "keywords": [
    "udacity",
    "performance",
    "optimisation",
    "optimization",
    "rendering",
    "Conor Doyle",
    "chocobuckle"
  ],
  "bugs": {
    "url": "https://github.com/chocobuckle/performance-optimisation/issues"
  },
  "homepage": "https://github.com/chocobuckle/performance-optimisation#readme",
  "dependencies": {},
  "devDependencies": {
    "clean-css-cli": "^4.0.9",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-webp": "^4.0.0",
    "juice": "^4.0.2",
    "lwip": "0.0.9"
  }
}
